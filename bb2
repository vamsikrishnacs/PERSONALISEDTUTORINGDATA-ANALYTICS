use amazonserver;
select * from (select username,pid,max(attempts),min(attempts),max(time),min(time),sum(grade),sum(max_grade) as sg1 from(select username,problem_id,attempts,event_type,grade,max_grade,time,split(split(split(path,"\\;")[5],"_")[1],"/")[0] as pid from pi_log_table where course_id='IITBombayX/CS101.1x/2T2014' order by username,time)t1 where pid!='null' group by username,pid)t2 where sg1>0;
