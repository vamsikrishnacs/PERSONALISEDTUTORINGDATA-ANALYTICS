use amazonserver;
select username,id,sum(result) from (select username,id,event_type,time,old_time,new_time,new_time-old_time as result
from  ( select username,id,event_type,time,old_time,new_time from vi_log_table where course_id='IITBombayX/CS101.1x/2T2014' order by time ) t2 where event_type='seek_video') t1 group by username,id;
